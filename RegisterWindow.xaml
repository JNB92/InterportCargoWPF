<Window x:Class="InterportCargoWPF.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Register" Height="350" Width="400">
    <Grid>
        <StackPanel Margin="10">
            <Label Content="First Name" />
            <TextBox x:Name="FirstNameBox" />

            <Label Content="Last Name" />
            <TextBox x:Name="LastNameBox" />

            <Label Content="Email" />
            <TextBox x:Name="EmailBox" />

            <Label Content="Phone Number" />
            <TextBox x:Name="PhoneNumberBox" />

            <Label Content="Password" />
            <PasswordBox x:Name="PasswordBox" />

            <Button Content="Register" Width="100" Click="RegisterButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
